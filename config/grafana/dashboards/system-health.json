{
  "dashboard": {
    "title": "HyperAgent - System Health",
    "tags": ["hyperagent", "system", "health"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Active Workflows",
        "type": "singlestat",
        "targets": [
          {
            "expr": "hyperagent_system_active_workflows",
            "legendFormat": "Active"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
        "valueName": "current"
      },
      {
        "id": 2,
        "title": "Queue Size",
        "type": "singlestat",
        "targets": [
          {
            "expr": "hyperagent_system_queue_size",
            "legendFormat": "Queued"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
        "valueName": "current"
      },
      {
        "id": 3,
        "title": "Database Connections",
        "type": "singlestat",
        "targets": [
          {
            "expr": "hyperagent_system_database_connections",
            "legendFormat": "Connections"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "valueName": "current"
      },
      {
        "id": 4,
        "title": "Redis Connections",
        "type": "singlestat",
        "targets": [
          {
            "expr": "hyperagent_system_redis_connections",
            "legendFormat": "Connections"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "valueName": "current"
      },
      {
        "id": 5,
        "title": "LLM API Latency (p95)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, hyperagent_llm_request_duration_seconds_bucket{provider=\"gemini\"})",
            "legendFormat": "Gemini"
          },
          {
            "expr": "histogram_quantile(0.95, hyperagent_llm_request_duration_seconds_bucket{provider=\"openai\"})",
            "legendFormat": "OpenAI"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4}
      },
      {
        "id": 6,
        "title": "LLM Token Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(hyperagent_llm_tokens_total{provider=\"gemini\"}[5m])",
            "legendFormat": "Gemini Tokens/sec"
          },
          {
            "expr": "rate(hyperagent_llm_tokens_total{provider=\"openai\"}[5m])",
            "legendFormat": "OpenAI Tokens/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4}
      },
      {
        "id": 7,
        "title": "Security Audit Results",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(hyperagent_audits_scans_total[5m])",
            "legendFormat": "Scans/sec"
          },
          {
            "expr": "rate(hyperagent_audits_vulnerabilities_total{severity=\"critical\"}[5m])",
            "legendFormat": "Critical Vulnerabilities"
          },
          {
            "expr": "rate(hyperagent_audits_vulnerabilities_total{severity=\"high\"}[5m])",
            "legendFormat": "High Vulnerabilities"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12}
      },
      {
        "id": 8,
        "title": "Deployment Success Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(hyperagent_deployments_total{status=\"success\"}[5m]) / rate(hyperagent_deployments_total[5m])",
            "legendFormat": "Success Rate"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12}
      },
      {
        "id": 9,
        "title": "Deployment Gas Costs",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, hyperagent_deployments_gas_used_bucket)",
            "legendFormat": "p95 Gas Used"
          },
          {
            "expr": "histogram_quantile(0.99, hyperagent_deployments_gas_used_bucket)",
            "legendFormat": "p99 Gas Used"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20}
      },
      {
        "id": 10,
        "title": "System Resource Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "process_resident_memory_bytes",
            "legendFormat": "Memory"
          },
          {
            "expr": "process_cpu_seconds_total",
            "legendFormat": "CPU"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20}
      }
    ]
  }
}

