{
  "dashboard": {
    "title": "HyperAgent - Agent Performance",
    "tags": ["hyperagent", "agents", "performance"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Agent Execution Duration (p95)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, hyperagent_agents_execution_duration_seconds_bucket{agent=\"generation\"})",
            "legendFormat": "Generation Agent"
          },
          {
            "expr": "histogram_quantile(0.95, hyperagent_agents_execution_duration_seconds_bucket{agent=\"audit\"})",
            "legendFormat": "Audit Agent"
          },
          {
            "expr": "histogram_quantile(0.95, hyperagent_agents_execution_duration_seconds_bucket{agent=\"testing\"})",
            "legendFormat": "Testing Agent"
          },
          {
            "expr": "histogram_quantile(0.95, hyperagent_agents_execution_duration_seconds_bucket{agent=\"deployment\"})",
            "legendFormat": "Deployment Agent"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Agent Execution Duration (p99)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.99, hyperagent_agents_execution_duration_seconds_bucket{agent=\"generation\"})",
            "legendFormat": "Generation Agent"
          },
          {
            "expr": "histogram_quantile(0.99, hyperagent_agents_execution_duration_seconds_bucket{agent=\"audit\"})",
            "legendFormat": "Audit Agent"
          },
          {
            "expr": "histogram_quantile(0.99, hyperagent_agents_execution_duration_seconds_bucket{agent=\"testing\"})",
            "legendFormat": "Testing Agent"
          },
          {
            "expr": "histogram_quantile(0.99, hyperagent_agents_execution_duration_seconds_bucket{agent=\"deployment\"})",
            "legendFormat": "Deployment Agent"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Agent Execution Count",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(hyperagent_agents_executions_total{agent=\"generation\"}[5m])",
            "legendFormat": "Generation Agent"
          },
          {
            "expr": "rate(hyperagent_agents_executions_total{agent=\"audit\"}[5m])",
            "legendFormat": "Audit Agent"
          },
          {
            "expr": "rate(hyperagent_agents_executions_total{agent=\"testing\"}[5m])",
            "legendFormat": "Testing Agent"
          },
          {
            "expr": "rate(hyperagent_agents_executions_total{agent=\"deployment\"}[5m])",
            "legendFormat": "Deployment Agent"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Agent Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(hyperagent_agents_errors_total{agent=\"generation\"}[5m])",
            "legendFormat": "Generation Agent"
          },
          {
            "expr": "rate(hyperagent_agents_errors_total{agent=\"audit\"}[5m])",
            "legendFormat": "Audit Agent"
          },
          {
            "expr": "rate(hyperagent_agents_errors_total{agent=\"testing\"}[5m])",
            "legendFormat": "Testing Agent"
          },
          {
            "expr": "rate(hyperagent_agents_errors_total{agent=\"deployment\"}[5m])",
            "legendFormat": "Deployment Agent"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "SLA Compliance (p95)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, hyperagent_agents_execution_duration_seconds_bucket{agent=\"generation\"}) < 60",
            "legendFormat": "Generation (SLA: 60s)"
          },
          {
            "expr": "histogram_quantile(0.95, hyperagent_agents_execution_duration_seconds_bucket{agent=\"audit\"}) < 100",
            "legendFormat": "Audit (SLA: 100s)"
          },
          {
            "expr": "histogram_quantile(0.95, hyperagent_agents_execution_duration_seconds_bucket{agent=\"testing\"}) < 100",
            "legendFormat": "Testing (SLA: 100s)"
          },
          {
            "expr": "histogram_quantile(0.95, hyperagent_agents_execution_duration_seconds_bucket{agent=\"deployment\"}) < 120",
            "legendFormat": "Deployment (SLA: 120s)"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 6,
        "title": "Agent Success Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(hyperagent_agents_executions_total{agent=\"generation\"}[5m]) / (rate(hyperagent_agents_executions_total{agent=\"generation\"}[5m]) + rate(hyperagent_agents_errors_total{agent=\"generation\"}[5m]))",
            "legendFormat": "Generation Agent"
          },
          {
            "expr": "rate(hyperagent_agents_executions_total{agent=\"audit\"}[5m]) / (rate(hyperagent_agents_executions_total{agent=\"audit\"}[5m]) + rate(hyperagent_agents_errors_total{agent=\"audit\"}[5m]))",
            "legendFormat": "Audit Agent"
          },
          {
            "expr": "rate(hyperagent_agents_executions_total{agent=\"testing\"}[5m]) / (rate(hyperagent_agents_executions_total{agent=\"testing\"}[5m]) + rate(hyperagent_agents_errors_total{agent=\"testing\"}[5m]))",
            "legendFormat": "Testing Agent"
          },
          {
            "expr": "rate(hyperagent_agents_executions_total{agent=\"deployment\"}[5m]) / (rate(hyperagent_agents_executions_total{agent=\"deployment\"}[5m]) + rate(hyperagent_agents_errors_total{agent=\"deployment\"}[5m]))",
            "legendFormat": "Deployment Agent"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      }
    ]
  }
}

